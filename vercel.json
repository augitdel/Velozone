{
  "functions": {
    "api/**": {
      "runtime": "@vercel/python",
      "excludeFiles": "{.next,*.cache,node_modules,public,app,todo.md,readme.md,image copy,image-1.png,datasets,tests,docs}/**"
    }
  },
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "settings": {
    "python": {
      "runtime": "3.9"
    }
  }
}
